{
  "ConnectionStrings": {
    "KuCloud": "Host=localhost;Port:5432;Database=KuCloud;Username=postgres;Password=XXX;SSL Mode=Disable"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/.log",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "CorsOption": {
    "AllowAnyOrigin": false,
    "AllowAnyMethod": false,
    "AllowAnyHeader": false
  }
}
